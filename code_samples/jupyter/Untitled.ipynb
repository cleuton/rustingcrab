    "// define uma função simples\n",
    "fn factorial(n: u64) -> u64 {\n",
    "    (1..=n).product()\n",
    "}\n",
    "\n",
    "// usa a função e imprime\n",
    "println!(\"5! = {}\", factorial(5));\n"
